{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "frontend/dist",
    "builds": [
      {
        "src": "backend/api/index.py",
        "use": "@vercel/python"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/api/index.py"
      },
      {
        "source": "/(.*)",
        "destination": "/frontend/dist/$1"
      }
    ],
    "env": {
      "PYTHON_VERSION": "3.9"
    }
  }