{
    "version": 2,
    "framework": "vite",
    "buildCommand": "cd frontend && npm install && npm run build",
    "outputDirectory": "frontend/dist",
    "functions": {
      "backend/api/index.py": {
        "runtime": "python3.9"
      }
    },
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/api/index.py"
      },
      {
        "source": "/(.*)",
        "destination": "/$1"
      }
    ],
    "env": {
      "PYTHON_VERSION": "3.9"
    },
    "regions": ["hkg1"]
  }